{% block style %}
<link rel="stylesheet" href="{{ static_url }}/index.css">
{% endblock %}

{% block body %}
{% include "./_partials/header.j2" %}

<main class="max-w-3xl mx-auto p-4">
{% for note in query.filter_notes() %}
<article>
  <h2>{{ note.title }}</h2>
  <div>{{ note.summary }}</div>
</article>
{% endfor %}
</main>

{% endblock %}
