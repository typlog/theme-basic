{
  "name": "basic",
  "version": "1.0.0",
  "repo": "typlog/theme-basic",
  "variables": {
    "color_mode": {
      "type": "enum",
      "default": "auto",
      "choices": ["auto", "light", "dark"]
    },
    "nav_links": {
      "type": "object[]",
      "required": false,
      "properties": {
        "title": {
          "type": "string"
        },
        "url": {
          "type": "url"
        },
        "blank": {
          "type": "boolean"
        }
      }
    }
  },
  "messages": {
    "archive": "Archive",
    "archive#zh": "归档"
  }
}
